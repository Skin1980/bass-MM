#include "apq8026-bass.dtsi"

&soc {
	earjack-debugger {
		status = "ok";
		lge,adc-low-threshold = <810>;
		lge,adc-high-threshold = <1000>;
		lge,trigger-mode = "low";
	};
};

&spmi_bus {
	qcom,pm8226@0 {
		qcom,power-on@800 {
			qcom,s3-src = "kpdpwr";
			qcom,pon_1 {
				qcom,support-reset = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <2000>;
				qcom,s2-type = <7>;
			};
			qcom,pon_3 {
				qcom,support-reset = <0>;
			};
		};
	};
};
