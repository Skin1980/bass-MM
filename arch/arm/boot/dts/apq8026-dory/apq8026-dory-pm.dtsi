&pm8226_vadc {
	chan@14 {
		label = "pa_therm0";
		reg = <0x14>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@17 {
		label = "pa_therm1";
		reg = <0x17>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@39 {
		label = "usb_id_lv";
		reg = <0x39>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <0>;
		qcom,fast-avg-setup = <0>;
	};

};

&soc {
	/* Cable type definition.
	* Modify the USB cable and move to SoC
	* to get each table for different USB Pull up resistor via HW Rev
	* This must have the compatible property "lge,usb-id".
	* If you don't want to use revision you can remove status & revision property.
	* If this node is not exist, the charger driver would not be probed.
	* Each cable type has 3 values
	* that we can configure the threshold of ADC value and TA/USB IUSB max current value.
	* ex)
	*    cable type = <adc_threshold, TA_MA, USB_MA>.
	*/

	lge,usb_cable@0 {
		compatible = "lge,usb-id";
		usb_adc_table@665{
			status = "okay";
			revision = "...rev_a";
			lge,no-init-cable = <100000 500 500>;
			lge,cable-56k = <200000 1500 1500>;
			lge,cable-100k = <239000 500 500>;
			lge,cable-130k = <340000 1500 1500>;
			lge,cable-180k = <400000 1100 500>;
			lge,cable-200k = <430000 1100 500>;
			lge,cable-220k = <485000 1100 500>;
			lge,cable-270k = <560000 1100 500>;
			lge,cable-330k = <735000 500 500>;
			lge,cable-620k = <955000 500 500>;
			lge,cable-910k = <1140000 1500 1500>;
			lge,cable-none = <1900000 1100 500>;
		};

		usb_adc_table@200{
			status = "okay";
			revision = "rev_b...";
			lge,no-init-cable = <197000 500 500>;
			lge,cable-56k = <496875 1500 1500>;
			lge,cable-100k = <654545 500 500>;
			lge,cable-130k = <780861 1500 1500>;
			lge,cable-180k = <876316 1100 500>;
			lge,cable-200k = <921429 1100 500>;
			lge,cable-220k = <988450 1100 500>;
			lge,cable-270k = <1077399 1100 500>;
			lge,cable-330k = <1240865 500 500>;
			lge,cable-620k = <1418326 500 500>;
			lge,cable-910k = <1637838 1500 1500>;
			lge,cable-none = <1900000 1100 500>;
		};
	};

	i2c@f9923000 {
		max17050@36 {
			compatible = "maxim,max17050";
			reg = <0x36>;
			interrupt-parent = <&msmgpio>;
			interrupts = <31 2>;

			maxim,rsns-microohm = <10000>;
			maxim,ext_batt_psy;
			maxim,full-soc = <99>;
			maxim,empty-soc = <0>;

			maxim,model = [A0 50 AA C0 AD 80 B4 10
					B5 00 B7 B0 B8 20 B8 90
					BC 30 BC 80 C3 40 C3 90
					CB 40 CC 80 D6 D0 D7 90
					00 C0 01 70 01 D0 04 40
					08 E0 2D B0 09 20 0C 20
					0C C0 11 20 0C C0 08 50
					06 60 06 90 0A A0 0A A0
					01 00 01 00 01 00 01 00
					01 00 01 00 01 00 01 00
					01 00 01 00 01 00 01 00
					01 00 01 00 01 00 01 00];

			maxim,relaxcfg = <0x506B>;
			maxim,config = <0x2100>;
			maxim,filtercfg = <0x87A4>;
			maxim,learncfg = <0x2606>;
			maxim,misccfg = <0x0870>;
			maxim,fullsocthr = <0x5F00>;
			maxim,rcomp0 = <0x004F>;
			maxim,tempco = <0x2F35>;
			maxim,tempnom = <0x1400>;
			maxim,tgain = <0xE932>;
			maxim,toff = <0x2381>;
			maxim,ichgterm = <0x00C0>;
			maxim,vempty = <0x964C>;
			maxim,qrtable00 = <0x0D84>;
			maxim,qrtable10 = <0x0A08>;
			maxim,qrtable20 = <0x0884>;
			maxim,qrtable30 = <0x0584>;
			maxim,capacity = <0x0320>;
			maxim,vf_fullcap = <0x0320>;
			maxim,temperature = <0x1400>;
			maxim,dpacc = <0x0C80>;
			maxim,param-version = <0x2>;
		};
	};

	battery_tm {
		compatible = "battery_tm";
		status = "ok";
		qcom,batt-tm-adc_tm = <&pm8226_adc_tm>;
		qcom,charger = <&pm8226_chg>;
		tm,low-vreg-mv = <4100>;
		tm,low-current-ma = <200>;
		tm,warm-cfg = <
			 (-50) (-100) (450) 0 1
			 (450)  (430) (550) 1 2
			 (550)  (430) (600) 2 2
			 (600)  (430) (790) 3 2>;
		tm,cool-cfg = <
			(-100) (-300) (-50) 2 7
			 (430) (-100) (450) 0 1>;
	};
};

&pm8226_chg {
	qcom,vddmax-mv = <4350>;
	qcom,vddsafe-mv = <4350>;
	qcom,vinmin-mv = <4450>;
	qcom,ibatmax-ma= <400>;
	qcom,ibatterm-ma = <30>;
	qcom,ibatsafe-ma = <400>;
	qcom,vbatdet-delta-mv = <100>;
	qcom,tchg-mins = <480>;
	qcom,thermal-mitigation = <0>; /* disable */
	qcom,btc-disabled;
	qcom,warm-bat-decidegc = <0>; /* disable */
	qcom,cool-bat-decidegc = <0>; /* disable */
	qcom,step-dwn-offset-ma = <200>;
	qcom,step-dwn-thr-mv = <4250>;
